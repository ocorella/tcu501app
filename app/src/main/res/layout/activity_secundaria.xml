<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="comsiteucrtcu501.google.httpssites.tcu501.SecundariaActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Secundaria"
        android:id="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:textSize="40dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7mo Grado"
        android:id="@+id/button_7mo"
        android:onClick="irA7mo"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="119dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8vo Grado"
        android:id="@+id/button_8vo"
        android:onClick="irA8vo"
        android:layout_below="@+id/button_7mo"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9no Grado"
        android:id="@+id/button_9no"
        android:onClick="irA9no"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/button_8vo"
        android:layout_toEndOf="@+id/button_8vo" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10mo Grado"
        android:id="@+id/button_10mo"
        android:onClick="irA10mo"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_9no"
        android:layout_centerHorizontal="true"
        android:layout_alignParentLeft="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11vo Grado"
        android:id="@+id/button_11vo"
        android:onClick="irA11vo"
        android:layout_below="@+id/button_9no"
        android:layout_toRightOf="@+id/button_7mo"
        android:layout_toEndOf="@+id/button_7mo" />


</RelativeLayout>
